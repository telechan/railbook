<table>
  <tr>
    <th>ISBNコード</th><th>書名</th><th>価格</th>
    <th>出版社</th><th>刊行日</th><th>ダウンロード</th>
  </tr>
<% @books.each do |book| %>
  <tr>
    <tb><%= book.isbn %></tb>
    <tb><%= book.title %></tb>
    <tb><%= book.price %></tb>
    <tb><%= book.publish %></tb>
    <tb><%= book.published %></tb>
    <tb><%= book.dl %></tb>
  </tr>
<% end %>
</table>